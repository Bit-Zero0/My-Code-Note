					"Children": [
						{
							"ID": "20220722140734-8ogyeuj",
							"Type": "NodeListItem",
							"ListData": {
								"BulletChar": 42,
								"Marker": "Kg=="
							},
							"Properties": {
								"id": "20220722140734-8ogyeuj",
								"updated": "20220722140734"
							},
							"Children": [
								{
									"ID": "20220722140734-5s2hqq5",
									"Type": "NodeParagraph",
									"Properties": {
										"id": "20220722140734-5s2hqq5",
