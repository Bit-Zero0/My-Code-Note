						"updated": "20220722140734"
					},
					"Children": [
						{
							"Type": "NodeText",
							"Data": "汇编阶段是把编译阶段生成的“"
						},
						{
							"Type": "NodeTextMark",
							"TextMarkType": "code",
							"TextMarkTextContent": ".s"
						},
						{
							"Type": "NodeText",
							"Data": "”文件转成目标文件\n读者在此可使用选项“"
						},
						{
							"Type": "NodeTextMark",
							"TextMarkType": "code",
							"TextMarkTextContent": "-c"
						},
						{
							"Type": "NodeText",
							"Data": "”就可看到汇编代码已转化为“"
						},
						{
							"Type": "NodeTextMark",
							"TextMarkType": "code",
							"TextMarkTextContent": ".o"
						},
						{
							"Type": "NodeText",
							"Data": "”的二进制目标代码了\n实例: "
						},
						{
							"Type": "NodeTextMark",
							"TextMarkType": "code",
							"TextMarkTextContent": "gcc –c hello.s –o hello.o"
						}
					]
				}
			]
		},
